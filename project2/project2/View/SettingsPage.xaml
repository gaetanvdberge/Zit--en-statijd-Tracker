<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="project2.View.SettingsPage"
                  xmlns:view="clr-namespace:project2.View">
    <MasterDetailPage.Master>
        <view:SideMenu x:Name="SideMenu" Title="SideMenu" />
    </MasterDetailPage.Master>

    <MasterDetailPage.Detail>
        <NavigationPage Title="title" BarTextColor="White" x:Name="NavPage">
            <x:Arguments>
                <view:SettingsDetail x:Name="SettingsDetail"/>

            </x:Arguments>
        </NavigationPage>

    </MasterDetailPage.Detail>
</MasterDetailPage>